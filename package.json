    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "ace-builds": "^1.4.12",
    "axios": "^0.21.1",
    "clone-deep": "^4.0.1",
    "flat": "^5.0.2",
    "mime": "^1.6.0",
    "node-sass": "^4.12.0",
    "query-string": "^6.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-json-view": "^1.21.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "store": "^2.0.12",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "add-license-header": "license-check-and-add add -f ./license-add-config.json",
    "check-license-header": "license-check-and-add check -f ./license-add-config.json"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/!(@awsui/components-react)"
    ]
  },
  "devDependencies": {
    "@types/clone-deep": "^4.0.1",
    "@types/flat": "^5.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/store": "^2.0.2",
    "@types/uuid": "^8.3.0",
    "csp-html-webpack-plugin": "^5.1.0",
    "customize-cra": "^1.0.0",
    "husky": "^4.3.7",
    "license-check-and-add": "^4.0.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-app-rewired": "^2.1.8"
  }
}
